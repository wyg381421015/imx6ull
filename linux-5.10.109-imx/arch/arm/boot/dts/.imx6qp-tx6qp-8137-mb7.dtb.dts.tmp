  regulator-min-microvolt = <3300000>;
  regulator-max-microvolt = <3300000>;
  pinctrl-names = "default";
  pinctrl-0 = <&pinctrl_lcd0_pwr>;
  gpio = <&gpio3 29 0>;
  enable-active-high;
  status = "disabled";
 };

 reg_lcd1_pwr: regulator-lcd1-pwr {
