  regulator-max-microvolt = <3300000>;
  pinctrl-names = "default";
  pinctrl-0 = <&pinctrl_flexcan_xcvr>;
  gpio = <&gpio4 21 1>;
 };

 reg_lcd0_pwr: regulator-lcd0-pwr {
