   status = "disabled";

   port@0 {
    reg = <0>;

    lvds0_mux_0: endpoint {
     remote-endpoint = <&ipu1_di0_lvds0>;
    };
   };

   port@1 {
    reg = <1>;

    lvds0_mux_1: endpoint {
     remote-endpoint = <&ipu1_di1_lvds0>;
    };
   };
  };

  lvds-channel@1 {
   #address-cells = <1>;
   #size-cells = <0>;
   reg = <1>;
   status = "disabled";

   port@0 {
    reg = <0>;

    lvds1_mux_0: endpoint {
