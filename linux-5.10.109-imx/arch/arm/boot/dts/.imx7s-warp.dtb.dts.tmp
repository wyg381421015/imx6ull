    clocks = <&clks 306>,
      <&clks 306>;
    clock-names = "ipg", "per";
    status = "disabled";
   };

   gpt3: timer@302f0000 {
    compatible = "fsl,imx7d-gpt", "fsl,imx6sx-gpt";
