  DOVDD-supply = <&sw2_reg>;
  DVDD-supply = <&sw2_reg>;
  AVDD-supply = <&reg_peri_3p15v>;

  port {
   ov2680_to_mipi: endpoint {
    remote-endpoint = <&mipi_from_sensor>;
    clock-lanes = <0>;
    data-lanes = <1>;
   };
  };
 };
};

