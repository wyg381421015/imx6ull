    interrupts = <0 110 4>;
    clocks = <&clks 137>,
      <&clks 138>;
    clock-names = "ipg", "per";
    fsl,stop-mode = <&gpr 0x10 1 0x10 17>;
    status = "disabled";
   };

   flexcan2: can@2094000 {
    compatible = "fsl,imx6sx-flexcan", "fsl,imx6q-flexcan";
    reg = <0x02094000 0x4000>;
    interrupts = <0 111 4>;
    clocks = <&clks 139>,
      <&clks 140>;
    clock-names = "ipg", "per";
