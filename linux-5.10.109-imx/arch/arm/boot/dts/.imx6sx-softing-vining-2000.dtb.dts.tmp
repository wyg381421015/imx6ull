   };

   pwm1: pwm@2080000 {
    compatible = "fsl,imx6sx-pwm", "fsl,imx27-pwm";
    reg = <0x02080000 0x4000>;
    interrupts = <0 83 4>;
    clocks = <&clks 186>,
      <&clks 186>;
    clock-names = "ipg", "per";
    #pwm-cells = <3>;
   };

   pwm2: pwm@2084000 {
    compatible = "fsl,imx6sx-pwm", "fsl,imx27-pwm";
    reg = <0x02084000 0x4000>;
    interrupts = <0 84 4>;
    clocks = <&clks 187>,
