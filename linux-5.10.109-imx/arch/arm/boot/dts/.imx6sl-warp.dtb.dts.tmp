    #interrupt-cells = <3>;
    interrupts = <0 89 4>;
    interrupt-parent = <&intc>;
    clocks = <&clks 63>;
    clock-names = "ipg";

    pgc {
     #address-cells = <1>;
     #size-cells = <0>;

     power-domain@0 {
      reg = <0>;
      #power-domain-cells = <0>;
     };

     pd_pu: power-domain@1 {
      reg = <1>;
      #power-domain-cells = <0>;
      power-supply = <&reg_pu>;
      clocks = <&clks 105>,
               <&clks 80>;
     };

     pd_disp: power-domain@2 {
      reg = <2>;
      #power-domain-cells = <0>;
      clocks = <&clks 113>,
        <&clks 114>,
        <&clks 112>,
        <&clks 115>,
        <&clks 111>;
     };
    };
   };

   gpr: iomuxc-gpr@20e0000 {
