   usbphy1: usbphy@20c9000 {
    compatible = "fsl,imx6sl-usbphy", "fsl,imx23-usbphy";
    reg = <0x020c9000 0x1000>;
    interrupts = <0 44 4>;
    clocks = <&clks 10>;
