    interrupts = <0 39 4>;
    clocks = <&clks 114>,
      <&clks 113>,
      <&clks 0>;
    clock-names = "pix", "axi", "disp_axi";
    status = "disabled";
    power-domains = <&pd_disp>;
   };

   dcp: crypto@20fc000 {
    compatible = "fsl,imx6sl-dcp", "fsl,imx28-dcp";
    reg = <0x020fc000 0x4000>;
    interrupts = <0 99 4>,
