     reg = <0x02028000 0x4000>;
     interrupts = <0 97 4>;
     clocks = <&clks 179>,
       <&clks 178>,
       <&clks 0>, <&clks 0>;
     clock-names = "bus", "mclk1", "mclk2", "mclk3";
     dmas = <&sdma 35 24 0>,
            <&sdma 36 24 0>;
