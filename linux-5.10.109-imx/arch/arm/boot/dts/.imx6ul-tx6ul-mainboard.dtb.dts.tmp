     interrupts = <0 98 4>;
     clocks = <&clks 181>,
       <&clks 180>,
       <&clks 0>, <&clks 0>;
     clock-names = "bus", "mclk1", "mclk2", "mclk3";
     dmas = <&sdma 37 24 0>,
